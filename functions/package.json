    "express": "^5.1.0",
    "firebase-admin": "^12.7.0",
    "firebase-functions": "^6.4.0",
    "gaxios": "^5.1.3",
    "gcp-metadata": "^5.3.0",
    "https-proxy-agent": "^5.0.1",
    "mongoose": "^8.17.0",
